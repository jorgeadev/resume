# /etc/nsswitch.conf
#
# Example configuration of GNU Name Service Switch functionality.
# If you have the `glibc-doc-reference' and `info' packages installed, try:
# `info libc "Name Service Switch"' for information about this file.

# pre_auth-client-config # passwd:         compat ldap
passwd:         compat sss ldap
# pre_auth-client-config # group:          compat ldap
group:          compat sss ldap
# pre_auth-client-config # shadow:         compat ldap
shadow:         compat
gshadow:        files

hosts:          files dns
networks:       files

protocols:      db files
services:       db files
ethers:         db files
rpc:            db files

# pre_auth-client-config # netgroup:       nis ldap
netgroup:       nis ldap
sudoers:        files ldap
automount:      files ldap
